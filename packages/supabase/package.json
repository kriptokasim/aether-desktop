{
    "name": "@aether/supabase",
    "description": "Supabase client library for Aether",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "clean": "rm -rf node_modules",
        "lint": "eslint --fix .",
        "format": "prettier --write .",
        "db:gen": "supabase gen types --lang=typescript --project-id $PROJECT_ID --schema public > src/types/db.ts",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "aether",
        "supabase"
    ],
    "author": {
        "name": "Aether",
        "email": "contact@aether.com"
    },
    "license": "Apache-2.0",
    "homepage": "https://aether.com",
    "dependencies": {
        "@supabase/supabase-js": "^2.45.6",
        "react": "^18.3.1"
    },
    "devDependencies": {
        "@aether/typescript": "*",
        "supabase": "^2.2.1",
        "typescript": "^5.5.4"
    },
    "exports": {
        "./clients": "./src/clients/index.ts",
        "./queries": "./src/queries/index.ts",
        "./rpc": "./src/rpc/index.ts",
        "./types": "./src/types/index.ts"
    }
}
