{
    "name": "@aether/nextjs",
    "version": "2.1.1",
    "description": "SWC plugin for aether",
    "repository": {
        "type": "git",
        "url": "https://github.com/aether-dev/desktop.git"
    },
    "main": "plugin.wasm",
    "scripts": {
        "dev": "nodemon --watch transform --exec 'npm run build'",
        "cpy": "cp target/wasm32-wasi/release/swc_plugin_aether.wasm ./plugin.wasm",
        "build": "cargo build --release --target wasm32-wasi && npm run cpy"
    },
    "homepage": "https://aether.com",
    "keywords": [
        "swc",
        "preprocessor",
        "aether"
    ],
    "preferUnplugged": true,
    "author": "aether",
    "devDependencies": {
        "nodemon": "^3.1.4"
    }
}